<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regression TestCases Suite">
    <listeners>
        <listener class-name="ExtentReporter.Listeners"/>
    </listeners>

    <test name="Verify user is able to register and login">
        <classes>
            <class name="TestCases.VerifyUserIsAbleToRegisterAndLogin"/>
        </classes>
    </test>

    <test name="Verify user is able to add product and checkout">
        <classes>
            <class name="TestCases.VerifyUserIsAbleToAddProductAndCheckout"/>
        </classes>
    </test>
</suite>